{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "array-bracket-spacing": "warn",
    "indent": [
      "warn",
      2
    ],
    "no-unused-vars": "warn",
    "semi": [
      2,
      "always"
    ]
  }
}